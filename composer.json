{
    "name": "marcel-mathias-nolte/contao-mobile-detection-content",
    "description": "MobileContent extension for Contao Open Source CMS",
    "keywords": ["contao", "mobile", "content"],
    "type": "contao-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "derhaeuptling",
            "homepage": "https://derhaeuptling.com/",
            "role":"Developer"
        },
        {
            "name": "Kamil Kuzminski",
            "email":"kamil.kuzminski@codefog.pl",
            "homepage": "https://codefog.pl",
            "role":"Developer"
        },
        {
            "name":"Glen Langer",
            "homepage":"http://contao.ninja",
            "role":"Developer"
        },
        {
            "name": "Serban Ghita",
            "email": "serbanghita@gmail.com",
            "homepage": "http://mobiledetect.net",
            "role": "Developer"
        },
        {
            "name": "Marcel Mathias Nolte",
            "homepage": "https://github.com/marcel-mathias-nolte",
            "role":"Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "contao/core-bundle":"^4.4",
        "contao/faq-bundle": "^4.4",
        "contao/news-bundle": "^4.4",
        "contao/calendar-bundle": "^4.4",
        "contao-community-alliance/composer-plugin":"~2.4 || ~3.0",
        "codefog/contao-haste":"^4.17",
        "mobiledetect/mobiledetectlib": "2.*"
    },
    "conflict": {
        "contao/core": "*",
        "derhaeuptling/contao-mobilecontent": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoMobilecontentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoMobilecontentBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "MarcelMathiasNolte\\ContaoMobilecontentBundle\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/marcel-mathias-nolte/contao-mobile-detection-content/issues",
        "source": "https://github.com/marcel-mathias-nolte/contao-mobile-detection-content"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
